App.js:
// App.js
import React from 'react';
import TableComponent from './TableComponent';
import PhotoComponent from './PhotoComponent';
import DetailsComponent from './DetailsComponent';
const App = () => {
return (
<div>
<h1>Static page</h1>
<TableComponent
photo={<PhotoComponent />}
details={<DetailsComponent />}
/>
</div>
);
};
export default App;
